<toast activationType="foreground" launch="args">
                    <visual>
                        <binding template="ToastGeneric">
                            <text>Action - text</text>
                            <text>Make sure left button on the toast has the text "ok" on it, and the right button has the text "cancel" on it.</text>
                        </binding>
                    </visual>
                    <actions>

                        <action content="ok" activationType="foreground" arguments="check"/>

                        <action content="cancel" activationType="foreground" arguments="cancel"/>

                    </actions>
                </toast>